    <connection>sakila</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>SELECT
  r.rental_id
, r.rental_date as data_aluguel
, f.title as filme
, concat(c.first_name,' ',c.last_name) as cliente
, return_date as data_retorno
, concat(s.first_name,' ', s.last_name) as funcionario
, p.amount as valor_aluguel
FROM 
    sakila.rental r
    inner join sakila.customer c on
        c.customer_id = r.customer_id
	inner join sakila.staff s on
        s.staff_id = r.staff_id
	inner join sakila.inventory i on
        i.inventory_id = r.inventory_id
    inner join sakila.film f on
        f.film_id = i.film_id
    inner join sakila.payment p on
        p.rental_id = r.rental_id
where rental_date between '${dtini}' and '${dtfim}'
</sql>
    <variables_active>Y</variables_active>
    <attributes/>
    <GUI>
      <xloc>368</xloc>
      <yloc>160</yloc>
    </GUI>
